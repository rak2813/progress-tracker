{
    "version": 2,
    "builds": [
      {
        "src": "project-tracker/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/project-tracker/dist/main.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri"
    }
  }